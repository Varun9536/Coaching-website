extends base.pug

block scripts 
    script(src= "../static/javascript.js") 

block style
    style
        include ../static/style.css 
        include ../static/contact.css 

block content
    div.container
        h1 Contact Us
        form(action = "/contact" method = "post" class = "myform")
            input(type = "text"  class = "myinput"  name = "name" placeholder = "Enter your name")
            input(type = "text"  class = "myinput"  name = "gender" placeholder = "Enter your gender")
            input(type = "text"  class = "myinput"  name = "email" placeholder = "Enter your Email id")
            input(type = "text"  class = "myinput"  name = "contact" placeholder = "Enter your contact no ")
            input(type = "text"  class = "myinput"  name = "address" placeholder = "Enter your address")
            button#btn 
                |   Submit